# Dictionary properties.
fsa.dict.separator=_
fsa.dict.encoding=utf-8
fsa.dict.encoder=SUFFIX

#fsa.dict.speller.equivalent-chars=
fsa.dict.speller.replacement-pairs=ent ement,gu g,f ph,ph f,c qu,qu c,k qu,qu k,x ct,ct x,bb b,b bb,cc c,c cc,ff f,f ff,ll l,l ll,mm m,m mm,nn n,n nn,pp p,p pp,rr r,r rr,ss s,s ss,ss c,c ss,ss ç,ç ss,tt t,t tt,œ oe,oe œ,æ ae,ae æ,ai é,é ai,ai è,è ai,ai ê,ê ai,ei é,é ei,ei è,è ei,ei ê,ê ei,o au,au o,o eau,eau o,ett èt,èt ett,ell èl,èl ell,t th,th t

fsa.dict.speller.runon-words=true
fsa.dict.speller.ignore-all-uppercase=false
fsa.dict.speller.ignore-camel-case=false
fsa.dict.speller.ignore-numbers=false
fsa.dict.speller.ignore-punctuation=true
# see issue #912 for why this should be true, even though it's slower:
fsa.dict.speller.ignore-diacritics=true
#It was commented out. Why?
fsa.dict.speller.convert-case=true

#TODO: to be added 
fsa.dict.frequency-included=true
